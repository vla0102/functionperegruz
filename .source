void AddRandom()
{
	int nextNumber = (rand() % 2 + 1) * 2;//генерируем числа от 1 до 2 и умножаем на 2
	int x, y;
	while (true)
	{
		x = rand() % 4;
		y = rand() % 4;
		if (data[y][x] == 0)//проверяе если в ячейке ноль то записывается сгенерированое число
		{
			data[y][x] = nextNumber;
			break;
		}
		
	}
}
bool IsEnd()
{
	for (int i = 0; i < 4; i++)
	{
		for (int j = 0; j < 4; j++)
		{
			if (data[i][j] == 0)
				return false;
			if (i<3 && data[i][j] == data[i + 1][j])
				return false;
			if (i<3 && data[j][i+1] == data[j][i])
				return false;
		}
	}
	return true;
}

bool Move(int dir)
{
	bool isMoved = false;
	for (int i = 0; i < 4;i++)
	if (MoveLine(dir, i))
		isMoved = true;
	return isMoved;
}

bool MoveLine(int dir, int n)
{
	int begin = dir % 2 ? 3 : 0;
	int inc = dir % 2 ? -1 : 1;
 
	if(dir <= 2)
	{
		for ( int i = begin; i >= 0 && i <= 3; i += inc)
		{
			for(int j = i + inc ; j >= 0 && j <= 3; j += inc)
			{
				if (data [i][n] == data [j][n])
				{
					data [i][n] += data [j][n];
					data [j][n] = 0;
					i += inc;
					break;
				}
				else if (data [j][n] != 0)
				{
					break;
				}
			}
		}
		for (int i = begin; i >= 0 && i <= 3; i += inc) // Сдвигаем в лево
		{
			if (data[i][n] == 0)
			{
				for (int j = i; j >= 0 && j <= 3; j += inc)
				{
					if (data[j][n] != 0)
					{
						data[i][n] = data[j][n];
						data[j][n] = 0;
						break;
					}
				}
			}
		}
	}
	else
	{
	for ( int i = begin; i >= 0 && i <= 3; i += inc)
		{
			for(int j = i + inc ; j >= 0 && j <= 3; j += inc)
			{
				if (data[n][i] == data[n][j])
				{
					data[n][i] += data[n][j];
					data[n][j] = 0;
					i += inc;
					break;
				}
				else if (data[n][j] != 0)
				{
					break;
				}
			}
		}
		for (int i = begin; i >= 0 && i <= 3; i += inc) // Сдвигаем в лево
		{
			if (data[n][i] == 0)
			{
				for (int j = i; j >= 0 && j <= 3; j += inc)
				{
					if (data[n][j] != 0)
					{
						data[n][i] = data[n][j];
						data[n][j] = 0;
						break;
					}
				}
			}
		}
	}
return true;
}

